<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sa.JuiceBar">

    <application
        android:allowBackup="true"
        android:icon="@drawable/screen"
        android:label="Juice Bar"
        android:roundIcon="@drawable/screen"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.sa.JuiceBar.Register" />
        <activity android:name="com.example.sa.JuiceBar.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.sa.JuiceBar.MainActivity" />
        <activity android:name="com.example.sa.JuiceBar.OrderPage" />
    </application>

</manifest>